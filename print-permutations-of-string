#include <iostream>
#include <string>
using namespace std;

void printPermutations(string input,string ans=""){

    	/* Don't write main() function.
	 * Don't read input, it is passed as function argument.
	 * Print output as specified in the question
	*/
    if(input.size() == 0)
    {
        cout<<ans<<endl;
        return;
    }
    for(int i=0;i<input.size();i++) {
        int temp = input[0] ;
        input[0] = input[i] ;
        input[i] = temp ;
        
        printPermutations(input.substr(1),ans+input[0]) ;
    }
}
