int longestIncreasingSubsequence(int* arr, int n) {
	// Write your code here
    if(n == 0)
        return 0;
    
    int* ans = new int[n];
    for(int i=0;i<n;i++) {
        ans[i] = 1;
    }
    ans[0] = 1;
    int lis=1;
    for(int i=1;i<n;i++) {
        for(int j=i-1;j>=0;j--) {
            if(arr[i] >= arr[j]){
                ans[i] = max(ans[i], ans[j]+1);;
                //break;
            }
        }
        if(ans[i] > lis)
            lis = ans[i];
    }
    
    return lis;
}
